class test_simple
{
    function application_start()
    {
        i32:x = 1;
        i32:y = 10;
        i32:z = x + y;

        testIf(x,y,z);
        testIf(z,y,x);
    }

    function testIf(i32:x, i32:y, i32:z):i32 {
        if (x < y < z) {
            return 1;
        } else {
            return 0;
        }
    }
}
